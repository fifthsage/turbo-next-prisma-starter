else
  export NODE_PATH="/Users/fifthsage/Projects/turbo_next_prisma_starter/node_modules/.pnpm/next@15.1.1_@babel+core@7.28.0_babel-plugin-macros@3.1.0_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules/next/dist/bin/node_modules:/Users/fifthsage/Projects/turbo_next_prisma_starter/node_modules/.pnpm/next@15.1.1_@babel+core@7.28.0_babel-plugin-macros@3.1.0_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules/next/dist/node_modules:/Users/fifthsage/Projects/turbo_next_prisma_starter/node_modules/.pnpm/next@15.1.1_@babel+core@7.28.0_babel-plugin-macros@3.1.0_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules/next/node_modules:/Users/fifthsage/Projects/turbo_next_prisma_starter/node_modules/.pnpm/next@15.1.1_@babel+core@7.28.0_babel-plugin-macros@3.1.0_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules:/Users/fifthsage/Projects/turbo_next_prisma_starter/node_modules/.pnpm/node_modules:$NODE_PATH"
